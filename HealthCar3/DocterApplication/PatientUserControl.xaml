                        <Label Content="Chat" HorizontalAlignment="Left" VerticalAlignment="Bottom" FontSize="33"/>

                    </Grid>

                    <Grid Grid.Row="1" Grid.Column="0">
                        <StackPanel >
                            <ScrollViewer Height="300" >
                                <StackPanel Name="ChatView6"></StackPanel>
                            </ScrollViewer>
                            <TextBox Height="40" BorderBrush="Black" BorderThickness="2" Name="ChatBox6" KeyDown="OnKeyDownEvent"/>
                        </StackPanel>
                    </Grid>

                    <Grid Grid.Column="1" Grid.Row="0">
                        <Label Content="HeartRate" FontSize="20"/>

                        <lvc:CartesianChart Margin="30" Name="HeartRateChart6"/>

                    </Grid>

                    <Grid Grid.Column="1" Grid.Row="1">
                        <Label Content="Speed" FontSize="20"/>

                        <lvc:CartesianChart Margin="30" Name="SpeedChart6"/>

                    </Grid>
                </Grid>
            </Grid>
        </TabItem>
    </TabControl>

</UserControl>
