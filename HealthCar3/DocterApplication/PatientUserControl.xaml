             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 

             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             xmlns:local="clr-namespace:DocterApplication"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="1020" Background="Transparent">



    <TabControl>
        <TabItem Header="Bike 1">
            <Grid>
                <Grid >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="416"/>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="340"/>
                        <RowDefinition Height="340"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Column="0" Grid.Row="0">
                        <Grid Height="60" Width="300" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="- "  FontSize="30" Name="ClientLabel1"/>
                        </Grid>

                        <Grid Height="40" Width="300"  Margin="0,50,00,10" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="Heartrate: "  FontSize="20" HorizontalAlignment="Left"/>
                            <Grid HorizontalAlignment="Right" Height="40" Width="100" >
                                <Border BorderBrush="Black" BorderThickness="2" >
                                    <Label Content="0 BPM"  FontSize="20" HorizontalAlignment="Center" Name="HeartrateLabel1"/>
                                </Border>
                            </Grid>
                        </Grid>

                        <Grid Height="40" Width="300"  Margin="0,100,00,10" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="Heartrate Average: "  FontSize="20" HorizontalAlignment="Left"/>
                            <Grid HorizontalAlignment="Right" Height="40" Width="100" >
                                <Border BorderBrush="Black" BorderThickness="2" >
                                    <Label Content="0 BPM"  FontSize="20" HorizontalAlignment="Center" Name="HeartrateAverageLabel1"/>
                                </Border>
                            </Grid>
                        </Grid>

                        <Grid Height="40" Width="300"  Margin="0,150,00,10" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="Speed: "  FontSize="20" HorizontalAlignment="Left"/>
                            <Grid HorizontalAlignment="Right" Height="40" Width="100" >
                                <Border BorderBrush="Black" BorderThickness="2" >
                                    <Label Content="0 m/s"  FontSize="20" HorizontalAlignment="Center" Name="SpeedLabel1"/>
                                </Border>
                            </Grid>
                        </Grid>

                        <Grid Height="40" Width="300"  Margin="0,200,00,10" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="Speed Average: "  FontSize="20" HorizontalAlignment="Left"/>
                            <Grid HorizontalAlignment="Right" Height="40" Width="100" >
                                <Border BorderBrush="Black" BorderThickness="2" >
                                    <Label Content="0 m/s" FontSize="20" HorizontalAlignment="Center" Name="SpeedAverageLabel1"/>
                                </Border>
                            </Grid>
                        </Grid>

                        <Grid Height="40" Width="350"  Margin="0,250,00,10" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <Label Content="Resistance: "  FontSize="20" HorizontalAlignment="Left"/>
                            <Grid Margin="150,0,0,0" Height="40" Width="100" >
                                <Border BorderBrush="Black" BorderThickness="2" >
                                    <TextBox Text="0"  FontSize="20" HorizontalAlignment="Center" Name="ResistanceLabel1" TextAlignment="Center" Width="100" PreviewTextInput="NumberValidationTextBox"/>
                                </Border>

                            </Grid>
                            <Button Content="Update" Height="40" Width="50" HorizontalAlignment="Right" BorderBrush="Black" BorderThickness="2" Click="UpdateResistance" Name="ResButton1"/>
                        </Grid>

                        <Button Height="40" Width="100" Style="{StaticResource HomeButton}" HorizontalAlignment="Right" Content="Start Session" Background="Red" FontWeight="Heavy" VerticalAlignment="Top" Margin="40" Click="StartSession" Name="StartButton1"/>
                        <Button Height="40" Width="100" Style="{StaticResource HomeButton}" HorizontalAlignment="Right" Content="Stop Session" Background="Red" FontWeight="Heavy" VerticalAlignment="Top" Margin="40,100,40,40" Click="StopSession" Name="StopButton1"/>

                        <Label Content="Chat" HorizontalAlignment="Left" VerticalAlignment="Bottom" FontSize="33"/>

                    </Grid>

                    <Grid Grid.Row="1" Grid.Column="0">
                        <StackPanel >
                            <ScrollViewer Height="300" >
                                <StackPanel Name="ChatView1"></StackPanel>
                            </ScrollViewer>
                            <TextBox Height="40" BorderBrush="Black" BorderThickness="2" Name="ChatBox1" KeyDown="OnKeyDownEvent"/>
                        </StackPanel>
                    </Grid>

                    <Grid Grid.Column="1" Grid.Row="0">
                        <Label Content="HeartRate" FontSize="20"/>

                        <lvc:CartesianChart Margin="30" Name="HeartRateChart1" Series="{Binding SeriesCollection}"/>

                    </Grid>

                    <Grid Grid.Column="1" Grid.Row="1">
                        <Label Content="Speed" FontSize="20"/>

                        <lvc:CartesianChart Margin="30" Name="SpeedChart1"/>

                    </Grid>
                </Grid>
            </Grid>
